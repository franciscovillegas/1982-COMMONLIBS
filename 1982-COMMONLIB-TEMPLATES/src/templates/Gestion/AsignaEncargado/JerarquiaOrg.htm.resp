function redrawNode(foldersNode, doc, level, lastNode, leftSide)
{
		var i=0
		var x,y,z,levAnt,nodox,hijos
		
                //doc.write("<form name='form1' method='post' action='/servlet/GeneraIndicadores/'>\n")
		doc.write("<table border=0 cellspacing=0 cellpadding=0 >")
		doc.write("<tr><td valign = middle nowrap >")
		doc.write(leftSide)
		nodox=foldersNode
        levAnt=nodox[4]
        hijos=nodox[7]
		/*alert("name: " + nodox[3]+"\n"+
			  "nivel: " + levAnt)*/

		if (level>0)
			if (lastNode)
			{	for (x=0;x<=levAnt;x++)
					leftSide = leftSide + "<img src='/images/DataFolder/images_arbol/blank.gif' width=16 height=22>" 
                
    			if (hijos==1)//tiene hijos,imagen con +
                  doc.write("<img src='/images/DataFolder/images_arbol/angulo+.jpg' width=16 height=22>")
 				else//no tiene hijos,imagen con -
			      doc.write("<img src='/images/DataFolder/images_arbol/angulo-.jpg' width=16 height=22>")		
                
                
                for (x=1;x<=levAnt;x++) //cantidad de gifs de acuerdo a los nivel
					doc.write("<img src='/images/DataFolder/images_arbol/esta_si.jpg' width=16 height=22>")	
			}
			else
			{   if (hijos==1)//tiene hijos,imagen con +
   				  doc.write("<img src='/images/DataFolder/images_arbol/perpen+.jpg' width=16 height=22>")
				else//no tiene hijos,imagen con -
			      doc.write("<img src='/images/DataFolder/images_arbol/perpen-.jpg' width=16 height=22>")		
				for (x=1;x<=levAnt;x++)
					doc.write("<img src='/images/DataFolder/images_arbol/esta_si.jpg' width=16 height=22>")
				leftSide = leftSide + "<img src='/images/DataFolder/images_arbol/vertical.jpg' width=16 height=22>"
                for (x=0;x<=levAnt-1;x++)
					leftSide = leftSide + "<img src='/images/DataFolder/images_arbol/blank.gif' width=16 height=22>"
			}
		displayIconAndLabel(foldersNode, doc)
		doc.write("</table>\n")
		if (foldersNode.length > 5 && foldersNode[0])
		{
			if (!foldersNode[2])
			{   
				level=level+1
				for (i=8; i<foldersNode.length;i++){
			
 	                if (i==foldersNode.length-1){
						redrawNode(foldersNode[i], doc, level, 1, leftSide)
					}
					else
						redrawNode(foldersNode[i], doc, level, 0, leftSide)
    			}
			}
			else
			{
				for (i=8; i<foldersNode.length;i++)
				{
					doc.write("<table border=0 cellspacing=0 cellpadding=0 valign=center>")
					doc.write("<tr><td nowrap>")
					doc.write(leftSide)
					if (i==foldersNode.length - 1){
					    	for (x=0;x<=levAnt-1;x++)
			   				  leftSide = leftSide + "<img src='/images/DataFolder/images_arbol/blank.gif' width=16 height=22>" 
			
							if (hijos==1)//tiene hijos,imagen con +
							  doc.write("<img src='/images/DataFolder/images_arbol/angulo+.jpg' width=16 height=22>")
							else//no tiene hijos,imagen con -
							  doc.write("<img src='/images/DataFolder/images_arbol/angulo-.jpg' width=16 height=22>")		
							for (x=1;x<=levAnt;x++)
								doc.write("<img src='/images/DataFolder/images_arbol/esta_si.jpg' width=16 height=22>")	
					}
					else{
						if (hijos==1)//tiene hijos,imagen con +
						  doc.write("<img src='/images/DataFolder/images_arbol/perpen+.jpg' width=16 height=22>")
						else//no tiene hijos,imagen con -
						  doc.write("<img src='/images/DataFolder/images_arbol/perpen-.jpg' width=16 height=22>")		
						for (x=1;x<=levAnt;x++)
							doc.write("<img src='/images/DataFolder/images_arbol/esta_si.jpg' width=16 height=22>")
					}
					doc.write(foldersNode[i])
					//doc.write("</table>")
					doc.write("</table>")
				}
			}
	    }
}
